                    </li>


                </ul>
            </div>
        </div>
    </nav>
    <!-- Page Content -->
    <br />

    <table align="center" class="border border-dark" border="1">
        <tr>
            <td colspan="2" align="center"><h3>Detalles del cliente</h3></td>
        </tr>
        <!---->
        <tr>
            <td><label>Rut : </label></td>
