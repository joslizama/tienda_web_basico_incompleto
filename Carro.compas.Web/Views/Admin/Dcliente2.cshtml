        </tr>
        <tr>
            <td><label>Direccion : </label></td>
            <td><label>@Model.direccion</label></td>
        </tr>
        <tr>
            <td><label>Comuna : </label></td>
            <td><label>@Model.comuna</label></td>
        </tr>
        <tr>
            <td><label>Ciudad : </label></td>
            <td><label>@Model.ciudad</label></td>
        </tr>
        <tr>
            <td><label>Correo : </label></td>
